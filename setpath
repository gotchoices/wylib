#Setup to find our resources in their source folders
#Just for debugging/development usage
#usage: source setpath

export WYATTDB="$USER"
export TCLLIBPATH=". ~/share/devel"
export RUBYOPT="-I ~/share/devel/wyseman/lib -I ~/share/devel/mychips/lib"
#export WYLIB_SITELIB=mychipslib
export PATH="$PATH:~/share/devel/wyseman/bin:~/share/devel/mychips/bin"
export NODE_PATH="$HOME/share/devel/js:/usr/lib/node_modules:./node_modules"
