{
  "name": "wylib",
  "version": "1.0.8",
  "description": "WyattERP Application Builder Library",
  "main": "dist/wylib-bundle.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev-hot": "webpack-dev-server --hot --config build/webpack.test.conf.js",
    "dev-build": "webpack --mode development --watch --progress --config build/webpack.dist.conf.js",
    "build": "webpack --mode production --config build/webpack.dist.conf.js",
    "wysegi": "webpack --mode production --config build/webpack.wgi.conf.js",
    "tclpkg": "wmmkpkg wylib 0.35 tcltk",
    "preversion": "npm-run-all build wysegi && git add -A",
    "lint": "eslint --ext .js src"
  },
  "keywords": [
    "ERP",
    "database",
    "application"
  ],
  "author": "Kyle Bateman",
  "license": "MIT",
  "homepage": "https://github.com/gotchoices/wylib",
  "babel": {
    "presets": [
      "env"
    ]
  },
  "dependencies": {
    "element-resize-detector": "^1.2.0",
    "flatpickr": "^4.5.2",
    "interactjs": "^1.3.4",
    "mathjs": "^5.4.0",
    "slickgrid-es6": "^3.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^2.0.1",
    "eslint": "^5.10.0",
    "file-loader": "^2.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.15.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svg-url-loader": "^2.3.1",
    "url-loader": "^1.1.2",
    "vue": "^2.5.21",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-node-externals": "^1.7.2"
  }
}
